{
	"name": "engram",
	"version": "0.1.0",
	"private": true,
	"description": "Cheap-first evaluation runner and result store for multi-model routing",
	"author": "Kevin Frilot",
	"license": "MIT",
	"type": "module",
	"bin": {
		"engram": "./dist/cli.js"
	},
	"packageManager": "bun@1.3.9",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"dev": "bun run src/cli.ts",
		"lint": "biome check src",
		"lint:fix": "biome check --write src",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"commander": "^14.0.3",
		"drizzle-orm": "^0.44.7"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.2",
		"@types/bun": "^1.3.2",
		"drizzle-kit": "^0.31.7",
		"typescript": "^5.9.3"
	}
}
