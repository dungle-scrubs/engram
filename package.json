{
	"name": "engram",
	"version": "0.1.0",
	"private": true,
	"description": "Cheap-first evaluation runner and result store for multi-model routing",
	"author": "Kevin Frilot",
	"license": "MIT",
	"type": "module",
	"bin": {
		"engram": "./dist/cli.js"
	},
	"packageManager": "bun@1.3.9",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"dev": "bun run src/cli.ts",
		"format": "biome format --write src test",
		"format:check": "biome format src test",
		"lint": "biome check src test",
		"lint:fix": "biome check --write src test",
		"prepare": "husky",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"commander": "^14.0.3",
		"drizzle-orm": "^0.44.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.4",
		"@types/bun": "^1.3.2",
		"drizzle-kit": "^0.31.7",
		"husky": "^9.1.7",
		"typescript": "^5.9.3"
	}
}
